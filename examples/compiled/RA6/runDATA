#!/bin/bash

setName=$1
#target=/afs/naf.desy.de/user/s/schettle/scratch/AnalysisTrees/whole2011_RA6
target=/scratch/hh/current/cms/user/schettle/AnalysisTrees/whole2011_RA6
scriptLoc=/afs/naf.desy.de/user/s/schettle/macro/CMSSW_4_2_5/src/SUSYBSMAnalysis/DesySusy/examples/compiled/batch/
codeLoc=/afs/naf.desy.de/user/s/schettle/macro/CMSSW_4_2_5/src/SUSYBSMAnalysis/DesySusy/examples/compiled/whole2011_RA6

if [ "$setName" = "" ]; then
	echo "please define setName or cancel [Ctrl c]."
	read setName
fi

go="n"



if [ "$go" != "all" ]; then
    echo "run ${setName}_DoubleMuMay10? [y/n/all]"
    read go
fi
if [ "$go" = "y" -o "$go" = "all" ]; then
    mkdir ${target}/${setName}_DoubleMuMay10
    cd ${target}/${setName}_DoubleMuMay10
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleMu/Run2011A-May10ReReco-v1/ \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=6
fi


if [ "$go" != "all" ]; then
    echo "run ${setName}_DoubleMuPRv4? [y/n/all]"
    read go
fi
if [ "$go" = "y" -o "$go" = "all" ]; then
    mkdir ${target}/${setName}_DoubleMuPRv4
    cd ${target}/${setName}_DoubleMuPRv4
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleMu/Run2011A-PromptReco-v4/ \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=6
fi


if [ "$go" != "all" ]; then
    echo "run ${setName}_DoubleMu05Aug? [y/n/all]"
    read go
