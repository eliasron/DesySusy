    cd ${target}/${setName}_DoubleElectronPRv4
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleElectron/Run2011A-PromptReco-v4/ \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=6
fi


if [ "$go" != "all" ]; then
    echo "run ${setName}_DoubleElectron05Aug? [y/n/all]"
    read go
fi
if [ "$go" = "y" -o "$go" = "all" ]; then
    mkdir ${target}/${setName}_DoubleElectron05Aug
    cd ${target}/${setName}_DoubleElectron05Aug
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleElectron/Run2011A-05Aug2011-v1 \
