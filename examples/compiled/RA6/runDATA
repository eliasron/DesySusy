    cd ${target}/${setName}_DoubleMu05Aug
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleMu/Run2011A-05Aug2011-v1 \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=4
fi


if [ "$go" != "all" ]; then
    echo run ${setName}_DoubleMuPRv6"? [y/n/all]"
    read go
fi
if [ "$go" = "y" -o "$go" = "all" ]; then
    mkdir ${target}/${setName}_DoubleMuPRv6
    cd ${target}/${setName}_DoubleMuPRv6
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleMu/Run2011A-PromptReco-v6/ \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=4
fi


if [ "$go" != "all" ]; then
    echo run ${setName}_DoubleMuRunB"? [y/n/all]"
    read go
