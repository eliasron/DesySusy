    mkdir ${target}/${setName}_DoubleMuRunB
    cd ${target}/${setName}_DoubleMuRunB
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleMu/Run2011B-PromptReco-v1/ \
	${codeLoc}/myAnalysis ${codeLoc}/data_config/config.txt cleanUp nFiles=8
fi


######################### E L E C T R O N S ############################


if [ "$go" != "all" ]; then
    echo "run ${setName}_DoubleElectronMay10? [y/n/all]"
    read go
fi
if [ "$go" = "y" -o "$go" = "all" ]; then
    mkdir ${target}/${setName}_DoubleElectronMay10
    cd ${target}/${setName}_DoubleElectronMay10
    ${scriptLoc}/./runOnAll \
	/pnfs/desy.de/cms/tier2/store/user/schettle/nTuple11_v4/data/DoubleElectron/Run2011A-May10ReReco-v1/ \
